{
  "name": "ngx-mentions",
  "version": "0.1.0",
  "description": "Angular v2+ Mentions",
  "repository": {
    "type": "git",
    "url": "https://github.com/nth-cloud/ngx-mentions"
  },
  "author": "https://github.com/nth-cloud/ngx-mentions/graphs/contributors",
  "license": "MIT",
  "bugs": "https://github.com/nth-cloud/ngx-mentions/issues",
  "homepage": "https://github.com/nth-cloud/ngx-mentions#readme",
  "main": "src/index.ts",
  "engines": {
    "node": ">=6.9.4 <7.0.0",
    "npm": ">=3.10.7 <4.0.0"
  },
  "dependencies": {
    "@angular/common": "^5.2.11",
    "@angular/core": "^5.2.11",
    "@angular/forms": "^5.2.11"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^0.7.5",
    "@angular-devkit/build-ng-packagr": "^0.7.5",
    "@angular/cli": "^6.1.5",
    "@angular/compiler": "^5.2.11",
    "@angular/compiler-cli": "^5.2.11",
    "@angular/http": "^5.2.11",
    "@angular/platform-browser": "^5.2.11",
    "@angular/platform-browser-dynamic": "^5.2.11",
    "@angular/router": "^5.2.11",
    "@ng-bootstrap/ng-bootstrap": "^1.1.2",
    "@types/jasmine": "2.8.3",
    "@types/jasminewd2": "~2.0.3",
    "@types/node": "^10.12.18",
    "@types/prismjs": "^1.9.0",
    "bootstrap": "^4.2.1",
    "clang-format": "^1.2.4",
    "codelyzer": "^4.5.0",
    "copy-webpack-plugin": "^4.6.0",
    "core-js": "^2.6.1",
    "glob": "^7.1.3",
    "gulp": "^4.0.0",
    "gulp-clang-format": "^1.0.27",
    "gulp-conventional-changelog": "^2.0.9",
    "gulp-file": "^0.4.0",
    "gulp-gh-pages": "^0.5.4",
    "husky": "^0.14.3",
    "jasmine": "^3.3.1",
    "jasmine-core": "^2.99.1",
    "jasmine-growl-reporter": "^2.0.0",
    "jasmine-node": "^1.16.2",
    "jasmine-spec-reporter": "^4.2.1",
    "jquery": "^3.3.1",
    "karma": "^3.1.4",
    "karma-chrome-launcher": "^2.2.0",
    "karma-coverage-istanbul-reporter": "^2.0.4",
    "karma-firefox-launcher": "^1.1.0",
    "karma-jasmine": "^1.1.2",
    "karma-jasmine-html-reporter": "^0.2.2",
    "karma-sauce-launcher": "^2.0.2",
    "ng-packagr": "^2.4.5",
    "popper.js": "^1.14.6",
    "prismjs": "^1.15.0",
    "rxjs": "^5.5.12",
    "ts-node": "^7.0.1",
    "tsickle": "^0.26.0",
    "tslint": "^5.12.0",
    "typescript": "~2.6.2",
    "zone.js": "^0.8.26"
  },
  "scripts": {
    "commitmsg": "node misc/validate-commit.js",
    "build": "npm run m:build && npm run demo:build",
    "test": "npm run check-format && npm run m:lint && npm run m:test",
    "tdd": "ng test ngx-mentions --source-map false",
    "demo": "gulp generate-demo-statics && npm run demo:serve",
    "check-format": "gulp check-format",
    "saucelabs": "ng test ngx-mentions --karma-config src/karma.sauce.conf.js --source-map false --progress false",
    "api-doc:tdd": "jasmine-node misc/api-doc.spec.js --autotest --watch misc/api-doc-test-cases/*.ts misc/api-doc.js",
    "api-doc:test": "jasmine-node misc/api-doc.spec.js",
    "m:build": "ng build ngx-mentions --prod && npm run m:static",
    "m:static": "gulp statics",
    "m:lint": "ng lint ngx-mentions",
    "m:test": "ng test ngx-mentions --code-coverage --source-map false --progress false --watch false",
    "demo:serve": "ng serve demo --host 0.0.0.0",
    "demo:static": "gulp generate-demo-statics",
    "demo:build": "gulp generate-demo-statics && ng build demo --prod",
    "demo:deploy": "npm run demo:build && gulp demo-push",
    "clean": "npm run clean:dist & npm run clean:test & npm run clean:demo & npm run clean:demo:deploy",
    "clean:dist": "[[ ! -e dist/ ]] || rm -r dist/",
    "clean:test": "[[ ! -e coverage/ ]] || rm -r coverage/",
    "clean:demo": "[[ ! -e demo/dist/ ]] || rm -r demo/dist",
    "clean:demo:deploy": "[[ ! -e .publish/ ]] || rm -r .publish/",
    "ci": "npm run test && npm run build",
    "changelog": "gulp changelog"
  },
  "keywords": [
    "angular",
    "mention",
    "mentions",
    "highlight",
    "components"
  ]
}
