{
  "name": "@nth-cloud/ng-mentions",
  "version": "0.1.7",
  "description": "Angular v5+ components & directives for customizable mentions",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/nth-cloud/ng-mentions.git"
  },
  "author": "https://github.com/nth-cloud/ng-mentions/graphs/contributors",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/nth-cloud/ng-mentions/issues"
  },
  "homepage": "https://github.com/nth-cloud/ng-mentions#readme",
  "engines": {
    "npm": ">=3.10.7 <4.0.0",
    "yarn": ">=1.3.0 <2.0.0"
  },
  "scripts": {
    "commitmsg": "node misc/validate-commit.js",
    "build": "yarn build:lib",
    "test": "yarn check-format && yarn lint && yarn test:lib",
    "tdd": "ng test ng-mentions",
    "check-format": "gulp check-format",
    "saucelabs": "ng test ng-mentions --config src/karma.sauce.conf.js --progress false",
    "api-doc:tdd": "jasmine-node misc/api-doc.spec.js --autotest --watch misc/api-doc-test-cases/*.ts misc/api-doc.js",
    "api-doc:test": "jasmine-node misc/api-doc.spec.js",
    "build:lib": "ng-packagr -p src/ng-package.prod.json && yarn static",
    "test:lib": "ng test ng-mentions --code-coverage --progress false --watch false",
    "static": "gulp statics",
    "lint": "ng lint ng-mentions",
    "demo": "gulp generate-demo-statics && yarn demo:serve",
    "demo:serve": "ng serve demo --host 0.0.0.0",
    "demo:static": "gulp generate-demo-statics",
    "demo:build": "gulp generate-demo-statics && ng build demo --prod",
    "demo:deploy": "yarn demo:build && gulp demo-push",
    "clean": "yarn clean:dist & yarn clean:test & yarn clean:demo & yarn clean:demo:deploy && yarn clean:demo:plunks",
    "clean:dist": "([[ ! -e dist/ ]] || rm -r dist/) && ([[ ! -e dist.tgz ]] || rm dist.tgz)",
    "clean:test": "[[ ! -e coverage/ ]] || rm -r coverage/",
    "clean:demo": "[[ ! -e demo/dist/ ]] || rm -r demo/dist",
    "clean:demo:deploy": "[[ ! -e .publish/ ]] || rm -fr .publish/",
    "clean:demo:plunks": "[[ ! -e demo/src/public/app/components ]] || rm -r demo/src/public/app/components",
    "ci": "yarn test && yarn build",
    "changelog": "gulp changelog"
  },
  "dependencies": {},
  "devDependencies": {
    "@angular/cli": "~1.7.4",
    "@angular/common": "5.2.11",
    "@angular/compiler": "5.2.11",
    "@angular/compiler-cli": "5.2.11",
    "@angular/core": "5.2.11",
    "@angular/forms": "5.2.11",
    "@angular/http": "5.2.11",
    "@angular/platform-browser": "5.2.11",
    "@angular/platform-browser-dynamic": "5.2.11",
    "@angular/router": "5.2.11",
    "@ng-bootstrap/ng-bootstrap": "^1.1.2",
    "@types/jasmine": "~2.8.6",
    "@types/jasminewd2": "~2.0.3",
    "@types/node": "~6.0.60",
    "@types/prismjs": "^1.9.0",
    "autoprefixer": "^7.2.2",
    "bootstrap": "^4.2.1",
    "clang-format": "~1.0.32",
    "codelyzer": "^4.5.0",
    "core-js": "^2.6.1",
    "glob": "^7.1.3",
    "gulp": "^3.9.1",
    "gulp-clang-format": "1.0.23",
    "gulp-conventional-changelog": "^1.1.0",
    "gulp-file": "^0.3.0",
    "gulp-gh-pages": "^0.5.4",
    "husky": "^0.14.3",
    "ie-shim": "^0.1.0",
    "jasmine": "^3.3.1",
    "jasmine-core": "^3.0.0",
    "jasmine-growl-reporter": "^2.0.0",
    "jasmine-node": "^1.16.2",
    "jasmine-spec-reporter": "^4.2.1",
    "jquery": "^3.3.1",
    "karma": "^3.1.4",
    "karma-chrome-launcher": "^2.2.0",
    "karma-coverage-istanbul-reporter": "^2.0.4",
    "karma-firefox-launcher": "^1.1.0",
    "karma-jasmine": "^1.1.2",
    "karma-jasmine-html-reporter": "^0.2.2",
    "karma-sauce-launcher": "^2.0.2",
    "ng-packagr": "^3.0.6",
    "popper.js": "^1.14.6",
    "prismjs": "^1.15.0",
    "remap-istanbul": "^0.11.0",
    "run-sequence": "^1.2.2",
    "rxjs": "^5.5.12",
    "systemjs": "^0.19.40",
    "ts-node": "~4.1.0",
    "tsickle": "~0.27.3",
    "tslib": "^1.7.1",
    "tslint": "^5.12.0",
    "typescript": "~2.8.2",
    "zone.js": "^0.8.26"
  }
}
